    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.media",
        "sections.lead-form",
        "sections.large-video",
        "sections.hero",
        "sections.feature-rows-group",
        "sections.case-study-group",
        "sections.bottom-actions",
        "sections.featured-on",
        "links.link",
        "sections.products",
        "sections.faq-group"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "blocks",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
